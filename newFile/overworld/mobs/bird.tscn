script/source = "extends \"../scripts/mover.gd\"

var timer = 0
var flipX = false
var flipY = false


func _process(delta):
	timer += delta
	
	realVelocity.x = cos(timer) * -1 if flipX else 1
	realVelocity.y = sin(timer) * -1 if flipY else 1
	
	if (timer > PI):
		if (randi() % 2 == 0): flipX = !flipX
		if (randi() % 2 == 0): flipY = !flipY
		timer -= PI
"

