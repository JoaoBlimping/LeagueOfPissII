	global.addToInventory(\"quit\")
	global.addToInventory(\"save\")
	global.addToInventory(\"grease\")
	global.addToInventory(\"grease\")
	global.saveGame()


func oldGame(file):
	global.saveFile = file
	global.loadGame()
	global.enterAdventure(global.area)


func instructions():
	get_tree().change_scene(\"res://menus/instructions.tscn\")


func quit():
	get_tree().quit()
