script/source = "extends \"../../scripts/room.gd\"


func start():
	if (s(\"keyboard\")):
		yield(say(\"\"\"What the fuck!\"\"\",\"ben\"),S)
		yield(say(\"What?\",\"dave\"),S)
		yield(say(\"\"\"They're already at the fucking door waiting for us. I had definitely
		not anticipated things going like this,
		~
		I'm sorry but I'm just going to have to do this.\"\"\",\"ben\"),S)
		yield(animate(\"boop\"),A)
		
		yield(say(\"What the fuck? What are you doing that for?\",\"dave\"),S)
		yield(animate(\"transport\"),A)
		fade(\"robot/factory2\")
	else:
		yield(say(\"\"\"weird, there are some people hanging around outside\"\"\",\"ben\"),S)
		global.drive(\"agara\",\"robot\")"

